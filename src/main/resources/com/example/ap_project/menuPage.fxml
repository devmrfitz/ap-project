<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="480.0" prefWidth="900.0" styleClass="bodybg" stylesheets="@home.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ap_project.MenuPage">
   <children>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" text="Exit">
         <font>
            <Font size="13.0" />
         </font>
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </Button>
      <Button layoutX="477.0" layoutY="414.0" mnemonicParsing="false" onAction="#gameStart" text="Play">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Button>
      <Button layoutX="300.0" layoutY="414.0" mnemonicParsing="false" onAction="#loadGame" text="Load Game">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
